function describe(value: int64 | string) -> string {
    return match value {
        int64 => "int"
        string => "string"
    }
}

function main() -> nil {
    print(describe(7))
    print(describe("ok"))

    probe: int64 | string := 7
    if probe matches int64 {
        print("is int64")
    }

    return nil
}
