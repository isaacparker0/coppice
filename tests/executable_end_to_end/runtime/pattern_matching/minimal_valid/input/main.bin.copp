type Direction :: enum {
    North,
    South,
}

function describe(value: int64 | string) -> string {
    return match value {
        int64 => "int"
        string => "string"
    }
}

function main() -> nil {
    print(describe(7))
    print(describe("ok"))

    probe: int64 | string := 7
    if probe matches int64 {
        print("is int64")
    }

    heading: Direction := Direction.North
    if heading matches Direction.North {
        print("north? yes")
    }
    label := match heading {
        Direction.North => "north"
        Direction.South => "south"
    }
    print(label)

    return nil
}
