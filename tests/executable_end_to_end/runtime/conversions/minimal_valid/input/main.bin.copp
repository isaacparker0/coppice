function main() -> nil {
    value_as_string := string("ok")
    value_as_int64 := int64(7)
    value_as_boolean := boolean(false)

    if value_as_string == "ok" {
        print("string pass")
    } else {
        print("string fail")
    }
    if value_as_int64 == 7 {
        print("int64 pass")
    } else {
        print("int64 fail")
    }
    if not value_as_boolean {
        print("boolean pass")
    } else {
        print("boolean fail")
    }

    print(string(123))
    print(string(-45))
    print(string(0))
    print(string(true))
    print(string(nil))
    return nil
}
