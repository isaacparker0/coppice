function main() -> nil {
    mut values := [1, 2, 3]

    if values.length == 3 {
        print("length ok")
    } else {
        print("length bad")
    }

    first := values.get(0)
    if first == nil {
        print("first nil")
    } else {
        print("first ok")
    }

    missing := values.get(99)
    if missing == nil {
        print("missing nil")
    } else {
        print("missing present")
    }

    values.set(1, 7)
    updated := values.get(1)
    updatedText := match updated {
        n: int64 => string(n)
        nil => "updated nil"
    }
    print(updatedText)

    return nil
}
