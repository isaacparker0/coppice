function writeFirst(mut values: List[int64], value: int64) -> nil {
    values[0] = value
    return nil
}

function main() -> nil {
    mut values := [1, 2, 3]

    if values.length == 3 {
        print("length ok")
    } else {
        print("length bad")
    }

    first := values[0]
    if first == 1 {
        print("first ok")
    } else {
        print("first bad")
    }

    values[1] = 7
    updated := values[1]
    print(string(updated))
    writeFirst(values, 9)
    print(string(values[0]))

    return nil
}
