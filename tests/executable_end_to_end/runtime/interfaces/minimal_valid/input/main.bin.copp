import workspace/lib_a { Printable as APrintable, User as AUser }
import workspace/lib_b { Printable as BPrintable, User as BUser }

function renderA(value: APrintable) -> string {
    return value.toString()
}

function renderB(value: BPrintable) -> string {
    return value.label()
}

function passthroughA[T: APrintable](value: T) -> T {
    return value
}

function passthroughB[T: BPrintable](value: T) -> T {
    return value
}

function main() -> nil {
    print(renderA(AUser { name: "Ada" }))
    print(renderB(BUser { name: "Ben" }))

    print(renderA(passthroughA[AUser](AUser { name: "Ari" })))
    print(renderA(passthroughA(AUser { name: "Ava" })))
    print(renderB(passthroughB[BUser](BUser { name: "Bea" })))
    print(renderB(passthroughB(BUser { name: "Bo" })))
    return nil
}
