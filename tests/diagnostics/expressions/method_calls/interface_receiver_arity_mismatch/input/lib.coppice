type Incrementer :: interface {
    function bump(self, delta: int64) -> int64,
}

type Counter implements Incrementer :: struct {
    value: int64,
    function bump(self, delta: int64) -> int64 {
        return self.value + delta
    },
}

function run() -> int64 {
    incrementer: Incrementer := Counter { value: 1 }
    return incrementer.bump()
}
