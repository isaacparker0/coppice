type User :: struct {
    id: int64,
    function idValue(self) -> int64 {
        return self.id
    },
    function bump(mut self, delta: int64) -> int64 {
        self = User { id: self.id + delta }
        return self.id
    },
}

function run() -> int64 {
    mut user := User { id: 1 }
    value := user.idValue()
    user.bump(value)
    return user.idValue()
}
