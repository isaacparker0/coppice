function add(a: int64, b: int64) -> int64 {
    return a + b
}

function identity[T](value: T) -> T {
    return value
}

function identityConstrained[T: int64](value: T) -> T {
    return value
}

function applyMapper(mapper: function(int64) -> int64, value: int64) -> int64 {
    return mapper(value)
}

function plusOne(value: int64) -> int64 {
    return value + 1
}

function run() -> int64 {
    result := add(1, 2)
    genericResult := identity[int64](result)
    constrainedResult := identityConstrained[int64](genericResult)
    mappedResult := applyMapper(plusOne, constrainedResult)
    print("ok")
    return mappedResult
}
