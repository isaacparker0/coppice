function ops(a: int64, b: int64, c: boolean) -> boolean {
    x := a + b
    y := a - b
    z := a * b
    w := a / b
    mod := a % b
    eq := x == y
    neq := x != y
    lt := a < b
    lte := a <= b
    gt := a > b
    gte := a >= b
    ok := (eq or neq) and (lt or lte or gt or gte) and c
    return ok and (z == w or z != w) and (mod == mod)
}

function nilEq() -> boolean {
    maybeText: string | nil := nil
    return maybeText == nil and maybeText != "x"
}

function stringAdd(prefix: string, suffix: string) -> string {
    return prefix + suffix
}

function stringAddWithConversion(value: int64) -> string {
    return string(value) + "!"
}
