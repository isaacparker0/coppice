visible type Incrementer :: interface {
    function bump(self, delta: int64) -> int64,
}

visible type Token implements Incrementer :: struct {
    id: int64,
    function bump(self, delta: int64) -> int64 {
        return self.id + delta
    },
}

visible function makeToken(id: int64) -> Token {
    return Token { id: id }
}

visible function makeIncrementer(id: int64) -> Incrementer {
    return Token { id: id }
}
