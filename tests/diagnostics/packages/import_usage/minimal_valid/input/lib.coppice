import workspace/auth { Token, START_ID, makeToken, identity, Box }

function idOf(token: Token) -> int64 {
    return token.bump(0)
}

function run() -> int64 {
    token := makeToken(START_ID)
    bumped := token.bump(1)
    boxed := Box[int64] { value: identity[int64](bumped) }
    return idOf(token) + boxed.get()
}
