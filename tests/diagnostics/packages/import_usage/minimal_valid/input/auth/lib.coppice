visible type Token :: struct {
    id: int64,
    function bump(self, delta: int64) -> int64 {
        return self.id + delta
    },
}

visible type Box[T] :: struct {
    value: T,
    function get(self) -> T {
        return self.value
    },
}

visible type Maybe[T] :: T | nil

visible START_ID: int64 := 1

visible function makeToken(id: int64) -> Token {
    return Token { id: id }
}

visible function identity[T](value: T) -> T {
    return value
}
