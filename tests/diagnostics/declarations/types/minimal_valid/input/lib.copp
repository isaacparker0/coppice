type User :: struct {
    id: int64,
    name: string,
}

type Outcome :: User | string

type Direction :: enum {
    North,
    South,
    East,
    West,
}

type Box[T] :: struct {
    value: T,
}

type Boxable :: interface {
    function score(self) -> int64,
}

type ConstrainedBox[T: Boxable] :: struct {
    value: T,
}

type Maybe[T] :: T | nil
