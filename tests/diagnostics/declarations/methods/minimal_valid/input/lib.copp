type User :: struct {
    id: int64,
    function idValue(self) -> int64 {
        return self.id
    },
    function bump(mut self, delta: int64) -> int64 {
        self = User { id: self.id + delta }
        return self.id
    },
}

function run() -> int64 {
    mut user := User { id: 41 }
    user.bump(1)
    return user.idValue()
}
