function choose() -> int64 {
    if true {
        return 1
    }
    return 2
}

function chooseWithElse() -> int64 {
    if true {
        return 1
    } else {
        return 2
    }
}

function countToThree() -> int64 {
    mut x := 0
    for x < 3 {
        x = x + 1
        if x == 2 {
            continue
        }
    }
    return x
}

function firstForever() -> int64 {
    mut x := 0
    for {
        x = x + 1
        if x == 1 {
            break
        }
    }
    return x
}

function unwrapEquals(value: int64 | nil) -> int64 {
    if value == nil {
        return 0
    } else {
        return value
    }
}

function unwrapNotEquals(value: int64 | nil) -> int64 {
    if value != nil {
        return value
    } else {
        return 0
    }
}

function unwrapMatches(value: int64 | string) -> int64 {
    if value matches int64 {
        return value
    } else {
        return 0
    }
}
