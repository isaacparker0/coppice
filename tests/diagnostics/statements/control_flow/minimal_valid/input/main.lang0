function choose() -> int64 {
    if true {
        return 1
    }
    return 2
}

function chooseWithElse() -> int64 {
    if true {
        return 1
    } else {
        return 2
    }
}

function countToThree() -> int64 {
    mut x := 0
    for x < 3 {
        x = x + 1
    }
    return x
}

function firstForever() -> int64 {
    for {
        return 1
    }
    return 0
}
