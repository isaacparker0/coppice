type Named :: interface {
    function name(self) -> string,
}

type User :: struct {
    value: string,
    function name(self) -> string {
        return self.value
    },
}

type Box[T: Named] :: struct {
    value: T,
}

function run() -> string {
    box := Box[User] { value: User { value: "text" } }
    return box.value.name()
}
