type User :: struct {
    id: int64,
    function bump(mut self, delta: int64) -> int64 {
        self = User { id: self.id + delta }
        return self.id
    },
}

function touch(value: int64) -> int64 {
    return value
}

function main() -> int64 {
    mut user := User { id: 1 }
    user.bump(1)
    touch(user.id)
    return user.id
}
