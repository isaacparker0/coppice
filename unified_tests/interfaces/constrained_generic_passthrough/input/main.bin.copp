type Printable :: interface {
    function toString(self) -> string,
}

type User implements Printable :: struct {
    name: string,
    function toString(self) -> string {
        return self.name
    },
}

function passthrough[T: Printable](value: T) -> T {
    return value
}

function main() -> nil {
    print(passthrough[User](User { name: "Ari" }).toString())
    print(passthrough(User { name: "Ava" }).toString())
    return
}
