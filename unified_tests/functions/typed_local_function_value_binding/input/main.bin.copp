function apply(value: int64, mapper: function(int64) -> int64) -> int64 {
    return mapper(value)
}

function plusOne(value: int64) -> int64 {
    return value + 1
}

function main() -> nil {
    localMapper: function(int64) -> int64 := plusOne
    print(string(apply(7, localMapper)))
    return nil
}
